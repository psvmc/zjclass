{
  "name": "zjclass",
  "version": "1.0.0",
  "description": "互动课堂",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win --x64"
  },
  "build": {
    "appId": "cn.psvmc",
    "files": ["**/*", "static/*"],
    "asar": true,
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "devDependencies": {
    "electron": "^6.0.2",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2"
  }
}